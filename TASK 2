<div class="container">
  <span class="text first"><b>I'm a </b></span>
  <span class="text second"><b></b></span>
</div> 
<script>
// multi text typing
const text = document.querySelector(".second");
const textLoad = ()=>{
    setTimeout(() => {
        text.textContent = "FreeLancer"
    }, 0);
    setTimeout(() => {
        text.textContent = "Blogger"
    }, 5000);

    setTimeout(() => {
        text.textContent = "YouTuber"
    }, 11000);
}
textLoad();
</script>
